<script>
  const countdownEl = document.getElementById("countdown");
  const labelEl = document.getElementById("countdown-label");

  function updateCountdown() {
    const startDate = new Date('2025-12-15T00:00:00').getTime();
    const endDate = new Date('2026-01-01T23:59:59').getTime();
    const now = new Date().getTime();

    let diff;
    let label;

    // Before sale starts
    if (now < startDate) {
      diff = startDate - now;
      label = "Starts in:";
    }
    // Sale is active
    else if (now >= startDate && now <= endDate) {
      diff = endDate - now;
      label = "Expires in:";
    }
    // Sale has ended
    else {
      countdownEl.textContent = "Sale Ended üéÑ";
      labelEl.textContent = "";
      clearInterval(timer);
      return;
    }

    labelEl.textContent = label;

    const days = Math.floor(diff / (1000 * 60 * 60 * 24));
    const hours = Math.floor((diff / (1000 * 60 * 60)) % 24);
    const minutes = Math.floor((diff / (1000 * 60)) % 60);
    const seconds = Math.floor((diff / 1000) % 60);

    // Use fixed-width formatting for stable width
    countdownEl.textContent = `${String(days).padStart(2, '0')}d ${String(hours).padStart(2, '0')}h ${String(minutes).padStart(2, '0')}m ${String(seconds).padStart(2, '0')}s üéÑ`;
  }

  updateCountdown();
  const timer = setInterval(updateCountdown, 1000);
</script>
<script>
document.addEventListener("DOMContentLoaded", () => {
  // Find all Buy Now / Get Started buttons
  const buttons = document.querySelectorAll(".buy-now, .get-started, a[href='#contact']");

  // Your "Contact Us" section is the 3rd footer-section inside #contact
  const contactSection = document.querySelector("#contact .footer-section:nth-of-type(3)");

  if (!contactSection) {
    console.warn("Contact Us section not found!");
    return;
  }

  buttons.forEach(button => {
    button.addEventListener("click", e => {
      e.preventDefault();

      // Scroll smoothly to footer
      document.getElementById("contact").scrollIntoView({
        behavior: "smooth",
        block: "center"
      });

      // Trigger highlight animation
      contactSection.classList.remove("contact-highlight");
      void contactSection.offsetWidth; // Force reflow to restart animation
      contactSection.classList.add("contact-highlight");

      // Remove highlight after animation ends
      setTimeout(() => {
        contactSection.classList.remove("contact-highlight");
      }, 1800);
    });
  });
});
</script>
<script>
document.addEventListener("DOMContentLoaded", () => {
  const contactSection = document.querySelector("#contact .footer-section:nth-of-type(3)");
  if (!contactSection) return;

  // Select *all* possible Buy Now / Get Started buttons
  const buttons = document.querySelectorAll(
    "a[href='#contact'], button[onclick*='#contact'], a.btn, button.calc-btn"
  );

  buttons.forEach(btn => {
    btn.addEventListener("click", e => {
      e.preventDefault();

      // Scroll smoothly to the footer
      document.getElementById("contact").scrollIntoView({ behavior: "smooth", block: "center" });

      // Trigger highlight
      contactSection.classList.add("contact-highlight");
      setTimeout(() => contactSection.classList.remove("contact-highlight"), 1500);
    });
  });
});
</script>
<script>
(function () {
  // -- Safety: run after DOM ready
  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', init);
  } else init();

  function init() {
    const contactSection = document.querySelector('#contact .footer-section:nth-of-type(3)');
    if (!contactSection) {
      console.warn('DEBUG: contactSection not found -> selector #contact .footer-section:nth-of-type(3)');
      return;
    }

    // Ensure highlight CSS exists (adds minimal inline style if missing)
    const styleId = 'contact-highlight-style';
    if (!document.getElementById(styleId)) {
      const st = document.createElement('style');
      st.id = styleId;
      st.textContent = `
        .footer-section.contact-highlight {
          box-shadow: 0 0 24px 6px rgba(255, 107, 53, 0.55) !important;
          background-color: rgba(255,107,53,0.12) !important;
          border-radius: 12px !important;
          transition: box-shadow 0.3s ease;
        }
      `;
      document.head.appendChild(st);
    }

    // Helper: restart highlight animation
    function flashContact() {
      contactSection.classList.remove('contact-highlight');
      void contactSection.offsetWidth; // reflow to restart
      contactSection.classList.add('contact-highlight');
      setTimeout(() => contactSection.classList.remove('contact-highlight'), 1800);
    }

    // Robust matcher: element or its closest ancestor qualifies as "Buy Now / Get Started"
    function qualifies(el) {
      if (!el) return false;
      const nodeName = el.nodeName.toLowerCase();
      // only consider actionable elements
      if (!['a','button','input'].includes(nodeName) && !el.closest) return false;

      const candidate = el.closest('a, button, input, [role="button"]');
      if (!candidate) return false;

      const txt = (candidate.textContent || '').trim().toLowerCase();
      const cls = (candidate.className || '').toLowerCase();
      const href = candidate.getAttribute && candidate.getAttribute('href');
      const onclick = candidate.getAttribute && candidate.getAttribute('onclick');

      // conditions that indicate it's a Buy/Get button that should go to contact
      if (cls.includes('buy-now') || cls.includes('get-started')) return {el:candidate, reason:'class'};
      if (txt.includes('buy') || txt.includes('get started') || txt.includes('get started')) return {el:candidate, reason:'text'};
      if (href && href.includes('#contact')) return {el:candidate, reason:'href'};
      if (onclick && onclick.includes('#contact')) return {el:candidate, reason:'onclick'};
      // also match generic .btn where text contains buy
      if (cls.includes('btn') && txt.includes('buy')) return {el:candidate, reason:'btn+text'};
      return false;
    }

    // Global click listener (capture phase so we can stop default navigation early)
    document.addEventListener('click', function (ev) {
      const match = qualifies(ev.target);
      if (!match) return; // let it go

      // Prevent navigation side effects (anchors, inline onclicks)
      try { ev.preventDefault(); } catch (e) {}
      try { ev.stopPropagation(); } catch (e) {}

      // Debug log - remove later if desired
      console.log('DEBUG: Contact click matched by', match.reason, match.el);

      // perform smooth scroll and highlight
      document.querySelector('#contact').scrollIntoView({ behavior: 'smooth', block: 'center' });

      // If there are any other handlers that also navigate after a tiny delay, try to re-apply highlight on scrollend-ish
      // Primary highlight now:
      flashContact();

      // Also try to re-apply after 600ms in case another script changed DOM
      setTimeout(() => {
        if (!contactSection.classList.contains('contact-highlight')) {
          flashContact();
        }
      }, 600);

      // If link had an href target (like plain '#contact'), do not let browser handle it automatically.
      // If you *do* want the URL to change (add #contact to address bar), uncomment the next line:
      // history.replaceState(null, '', '#contact');
    }, true); // <-- capture = true
  }
})();
      // FAQ Toggle
document.querySelectorAll('.faq-question').forEach(question => {
    question.addEventListener('click', () => {
        const faqItem = question.parentElement;
        faqItem.classList.toggle('active');
    });
});
</script>
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Organization",
  "name": "The Finals Boosting Services",
  "url": "https://www.thefinalsboosting.com/",
  "logo": "https://i.imgur.com/bUHbKI0.png",
  "description": "Cheap, fast & reliable boosting in The Finals.",
  "contactPoint": {
    "@type": "ContactPoint",
    "contactType": "Customer Support",
    "email": "boostingthefinals@gmail.com"
  }
}
</script>
<script>
// Testimonials Carousel
const slider = document.querySelector('.testimonial-slider');
const cards = document.querySelectorAll('.testimonial-card');
const prevBtn = document.querySelector('.testimonial-nav.prev');
const nextBtn = document.querySelector('.testimonial-nav.next');
const dotsContainer = document.querySelector('.testimonial-dots');

let currentIndex = 0;
let cardsPerView = 3;

// Calculate cards per view based on screen size
function updateCardsPerView() {
  if (window.innerWidth <= 600) {
    cardsPerView = 1;
  } else if (window.innerWidth <= 900) {
    cardsPerView = 2;
  } else {
    cardsPerView = 3;
  }
}

// Create dots
function createDots() {
  dotsContainer.innerHTML = '';
  const totalDots = Math.ceil(cards.length / cardsPerView);
  
  for (let i = 0; i < totalDots; i++) {
    const dot = document.createElement('span');
    dot.classList.add('dot');
    if (i === 0) dot.classList.add('active');
    dot.addEventListener('click', () => goToSlide(i));
    dotsContainer.appendChild(dot);
  }
}

// Update carousel position
function updateCarousel() {
  const cardWidth = cards[0].offsetWidth;
  const gap = 16; // gap between cards
  const offset = currentIndex * (cardWidth + gap);
  slider.style.transform = `translateX(-${offset}px)`;
  
  // Update dots
  document.querySelectorAll('.dot').forEach((dot, index) => {
    dot.classList.toggle('active', index === currentIndex);
  });
  
  // Update button states
  prevBtn.disabled = currentIndex === 0;
  nextBtn.disabled = currentIndex >= cards.length - cardsPerView;
}

// Go to specific slide
function goToSlide(index) {
  currentIndex = index;
  updateCarousel();
}

// Next slide
function nextSlide() {
  if (currentIndex < cards.length - cardsPerView) {
    currentIndex++;
    updateCarousel();
  }
}

// Previous slide
function prevSlide() {
  if (currentIndex > 0) {
    currentIndex--;
    updateCarousel();
  }
}

// Event listeners
prevBtn.addEventListener('click', prevSlide);
nextBtn.addEventListener('click', nextSlide);

// Handle resize
window.addEventListener('resize', () => {
  updateCardsPerView();
  currentIndex = 0;
  createDots();
  updateCarousel();
});

// Initialize
updateCardsPerView();
createDots();
updateCarousel();

        const bars = document.querySelectorAll('.bar');
        bars.forEach(bar => {
            bar.addEventListener('mouseenter', function() {
                const value = this.querySelector('.bar-value').textContent;
                const label = this.querySelector('.bar-label').textContent.replace(/\n/g, ' ');
                this.title = `${label}: ${value} orders completed`;
            });
        });

// World Tour Functionality
document.addEventListener('DOMContentLoaded', function() {
    let basePrice = 300;
    let methodPrice = 0;
    let speedPrice = 0;

    // Region buttons
    const regionBtns = document.querySelectorAll('.toggle-group .toggle-btn');
    regionBtns.forEach(btn => {
        btn.addEventListener('click', function() {
            regionBtns.forEach(b => b.classList.remove('active'));
            this.classList.add('active');
        });
    });

    // Method checkboxes
    const methodOptions = document.querySelectorAll('.checkbox-option[data-method]');
    methodOptions.forEach(option => {
        option.addEventListener('click', function() {
            methodOptions.forEach(o => o.classList.remove('selected'));
            this.classList.add('selected');
            methodPrice = this.dataset.method === 'selfplay' ? 50 : 0;
            updateWorldTourPrice();
        });
    });

    // Speed checkboxes  
    const speedOptions = document.querySelectorAll('.checkbox-option[data-speed]');
    speedOptions.forEach(option => {
        option.addEventListener('click', function() {
            speedOptions.forEach(o => o.classList.remove('selected'));
            this.classList.add('selected');
            if (this.dataset.speed === 'express') speedPrice = 75;
            else if (this.dataset.speed === 'super') speedPrice = 150;
            else speedPrice = 0;
            updateWorldTourPrice();
        });
    });

    // Rank sliders
    const currentRankInput = document.getElementById('currentRank');
    const desiredRankInput = document.getElementById('desiredRank');
    const fromSlider = document.getElementById('fromSlider');
    const toSlider = document.getElementById('toSlider');

    if (currentRankInput && desiredRankInput && fromSlider && toSlider) {
        currentRankInput.addEventListener('input', function() {
            fromSlider.value = this.value;
            updateWorldTourPrice();
        });

        desiredRankInput.addEventListener('input', function() {
            toSlider.value = this.value;
            updateWorldTourPrice();
        });

fromSlider.addEventListener('input', function() {
    if (parseInt(fromSlider.value) > parseInt(toSlider.value)) {
        fromSlider.value = toSlider.value;
    }
    currentRankInput.value = this.value;
    updateWorldTourPrice();
});

toSlider.addEventListener('input', function() {
    if (parseInt(toSlider.value) < parseInt(fromSlider.value)) {
        toSlider.value = fromSlider.value;
    }
    desiredRankInput.value = this.value;
    updateWorldTourPrice();
});

    }

    function updateWorldTourPrice() {
        const current = parseInt(currentRankInput.value) || 0;
        const desired = parseInt(desiredRankInput.value) || 2400;
        const rankDiff = Math.max(0, desired - current);
        const calculatedBase = Math.round((rankDiff / 2400) * basePrice);
        const total = calculatedBase + methodPrice + speedPrice;
        const cashback = Math.round(total * 0.05);

        document.getElementById('basePrice').textContent = '‚Ç¨' + calculatedBase;
        document.getElementById('methodPrice').textContent = '‚Ç¨' + methodPrice;
        document.getElementById('speedPrice').textContent = '‚Ç¨' + speedPrice;
        document.getElementById('finalPrice').textContent = '‚Ç¨' + total;
        document.getElementById('totalPrice').innerHTML = total + '<span class="price-currency">‚Ç¨</span>';
        document.getElementById('cashback').textContent = cashback + '‚Ç¨ GET BACK BY CASHBACK';
        
        const buyBtn = document.querySelector('.buy-button');
        if (buyBtn) buyBtn.textContent = 'Buy now per ‚Ç¨' + total;
    }

    updateWorldTourPrice();
});
</script>
</body>
</html>


















